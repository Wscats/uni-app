  "q_refresh": {
    "body": [
      "<refresh class=\"refresh\" onrefresh=\"refresh\" refreshing=\"{{isRefreshing}}\">",
      "\t$1<div class=\"page-title-wrap\">",
      "\t\t<text class=\"page-title\">{{componentName}}</text>",
      "\t</div>",
      "\t<div class=\"item-container\">",
      "\t\t<input class=\"btn\" type=\"button\" value=\"停止刷新\" onclick=\"stopRefresh\">",
      "\t</div>",
      "</refresh>"
    ],
    "triggerAssist": false,
    "prefix": "q_refresh"
  },

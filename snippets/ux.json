  "q_list": {
    "body": [
      "<list onscrollbottom=\"scrollbottom\" id=\"list\">",
      "\t<!-- 列表元素，属性type值相同时，需要确保渲染后的视图布局也完全相同 -->",
      "\t<list-item type=\"listItem\" for=\"{{listData$1}}\">",
      "\t\t<text>{{\\$item}}--{{\\$idx}}</text>",
      "\t</list-item>",
      "\t<!-- 加载更多 -->",
      "\t<list-item type=\"loadMore\" class=\"load-more\" if=\"{{loadMore}}\">",
      "\t\t<progress type=\"circular\"></progress>",
      "\t\t<text>加载更多</text>",
      "\t</list-item>",
      "</list>"
    ],
    "triggerAssist": false,
    "prefix": "q_list"
  },
